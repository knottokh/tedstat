<%= column_chart @graphdata,adapter: "highcharts",library: {
                    yAxis: {
                        labels: {
                            enabled: true,
                            style: {
                                fontSize:'22px'
                            }
                        },
                        min: 0, max: 3, tickInterval: 1
                    },
                    legend: {
                        itemStyle: {
                             fontSize: '22px'
